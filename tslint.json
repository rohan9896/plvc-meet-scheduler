{
  "extends": "tslint:recommended",
  "defaultSeverity": "error",
  "rules": {
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "trace"],
      "max-line-legth": [true, 100],
      "semicolon": {
        "option": ["always"]
      }
    }
  },
  "jsRules": {}
}
